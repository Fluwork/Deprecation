{
    "name": "fluwork/deprecation",
    "description": "Small library to trigger deprecations.",
    "keywords": ["deprecation"],
    "homepage": "https://github.com/Fluwork/Deprecation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Foucher",
            "homepage": "https://github.com/YaFou",
            "role": "Owner"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "dist": "*"
        }
    },
    "readme": ".github/README.md",
    "support": {
        "issues": "https://github.com/Fluwork/Deprecation/issues",
        "source": "https://github.com/Fluwork/Deprecation"
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "spatie/phpunit-watcher": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Fluwork\\Deprecation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fluwork\\Deprecation\\Tests\\": "tests/"
        }
    }
}